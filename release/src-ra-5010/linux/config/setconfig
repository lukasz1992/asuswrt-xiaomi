				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/4M_16M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_4M32M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/4M_32M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/4M_32M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_8M16M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/8M_16M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_16M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_8M32M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_32M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_8M32M_VOIP; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_voip_config.uclibc$TMP"
