				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_128M_config.modules$TMP"
			elif var_isset CONFIG_DEFSETTING_16M128M; then
				cp modules/.config "vendors/$VENDOR/$PRODUCT/config/16M_128M_config.modules$TMP"
