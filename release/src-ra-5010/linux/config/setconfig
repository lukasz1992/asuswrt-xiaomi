		elif var_isset CONFIG_DEFSETTING_8M32M; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_config.vendor$TMP"
			COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_32M_config.vendor$TMP"
		elif var_isset CONFIG_DEFSETTING_8M32M_VOIP; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_voip_config.vendor$TMP"
			COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_32M_voip_config.vendor$TMP"
		elif var_isset CONFIG_DEFSETTING_8M64M; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/8M_64M_config.vendor$TMP"
