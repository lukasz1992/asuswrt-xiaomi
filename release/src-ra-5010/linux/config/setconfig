			COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/16M_64M_config.vendor$TMP"
		elif var_isset CONFIG_DEFSETTING_32M64M; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/32M_64M_config.vendor$TMP"
			COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/32M_64M_config.vendor$TMP"
		elif var_isset CONFIG_DEFSETTING_8M128M; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/8M_128M_config.vendor$TMP"
			COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_128M_config.vendor$TMP"
		elif var_isset CONFIG_DEFSETTING_16M128M; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/16M_128M_config.vendor$TMP"
