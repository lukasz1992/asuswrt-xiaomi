			elif var_isset CONFIG_DEFSETTING_8M64M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/8M_64M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_64M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_16M64M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/16M_64M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/16M_64M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_32M64M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/32M_64M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/32M_64M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_8M128M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/8M_128M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_128M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_16M128M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/16M_128M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/16M_128M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_128M128M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/128M_128M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/128M_128M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_128M256M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/128M_256M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/128M_256M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_256M256M; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/256M_256M_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/256M_256M_config.uclibc$TMP"
			elif var_isset CONFIG_DEFSETTING_8GM1G; then
				cp lib/.config "vendors/$VENDOR/$PRODUCT/config/8G_1G_config.uclibc$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8G_1G_config.uclibc$TMP"
			fi
		fi
		if [ -f "uClibc-0.9.33.2/.config" ]; then
			if var_isset CONFIG_DEFSETTING_2M8M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/2M_8M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/2M_8M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_2M16M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/2M_16M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/2M_16M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_4M16M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/4M_16M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/4M_16M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_4M32M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/4M_32M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/4M_32M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_8M16M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/8M_16M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_16M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_8M32M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_32M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_8M32M_VOIP; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_voip_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_32M_voip_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_8M64M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/8M_64M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_64M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_16M64M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/16M_64M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/16M_64M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_32M64M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/32M_64M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/32M_64M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_8M128M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/8M_128M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_128M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_16M128M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/16M_128M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/16M_128M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_128M128M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/128M_128M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/128M_128M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_128M256M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/128M_256M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/128M_256M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_256M256M; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/256M_256M_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/256M_256M_config.uclibc0933$TMP"
			elif var_isset CONFIG_DEFSETTING_8G1G; then
				cp uClibc-0.9.33.2/.config "vendors/$VENDOR/$PRODUCT/config/8G_1G_config.uclibc0933$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8G_1G_config.uclibc0933$TMP"
			fi
		fi
		
		if [ -f "user/busybox/.config" ]; then
			if var_isset CONFIG_DEFSETTING_2M8M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/2M_8M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/2M_8M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_2M16M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/2M_16M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/2M_16M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_4M16M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/4M_16M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/4M_16M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_4M32M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/4M_32M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/4M_32M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_8M16M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/8M_16M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_16M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_8M32M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_32M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_8M32M_VOIP; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/8M_32M_voip_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_32M_voip_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_8M64M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/8M_64M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_64M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_16M64M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/16M_64M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/16M_64M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_32M64M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/32M_64M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/32M_64M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_8M128M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/8M_128M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8M_128M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_16M128M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/16M_128M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/16M_128M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_128M128M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/128M_128M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/128M_128M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_128M256M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/128M_256M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/128M_256M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_256M256M; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/256M_256M_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/256M_256M_config.busybox$TMP"
			elif var_isset CONFIG_DEFSETTING_8G1G; then
				cp user/busybox/.config "vendors/$VENDOR/$PRODUCT/config/8G_1G_config.busybox$TMP"
				COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/8G_1G_config.busybox$TMP"
			fi
			tar zcvf save_default.tgz $COMPRESSFILE
		fi

		sed 's/^\(CONFIG_DEFAULTS_VENDOR_UPDATE\)=y/# \1 is not set/' <.config\
				> .config.tmp
		cp .config.tmp .config
		rm -f .config.tmp

		if [ -f "vendors/$VENDOR/$PRODUCT/config.device" ]
		then
			cp .config "vendors/$VENDOR/$PRODUCT/config.device"
		fi
	fi

	grep -v CONFIG_DEFAULTS_VENDOR= < .config | \
		grep -v CONFIG_DEFAULTS_MODULES= | \
		grep -v CONFIG_DEFAULTS_KERNEL= > .config.tmp

	cp .config.tmp .config
	rm -f .config.tmp
fi

#############################################################################
exit 0
