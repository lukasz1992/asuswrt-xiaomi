		elif var_isset CONFIG_DEFSETTING_128M256M; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/128M_256M_config.vendor$TMP"
			COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/128M_256M_config.vendor$TMP"
		elif var_isset CONFIG_DEFSETTING_256M256M; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/256M_256M_config.vendor$TMP"
			COMPRESSFILE="$COMPRESSFILE vendors/$VENDOR/$PRODUCT/config/256M_256M_config.vendor$TMP"
		elif var_isset CONFIG_DEFSETTING_8G1G; then
			cp config/.config "vendors/$VENDOR/$PRODUCT/config/8G_1G_config.vendor$TMP"
